<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.axxesinternational.vias.axxes.fragments.DatosPlanoNuevoMantFragment">


    <TextView
        android:id="@+id/txt_folio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/primary"
        android:text="Datos de Plano con Folio"
    />

    <ListView
        android:id="@+id/my_list_view_datos_plano_nuevo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="false"
        android:longClickable="false"
        android:layout_below="@id/txt_folio"
    />


</RelativeLayout>
